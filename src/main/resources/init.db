DROP TABLE IF EXISTS groups CASCADE;
DROP TABLE IF EXISTS students;
DROP TABLE IF EXISTS courses;
CREATE TABLE IF NOT EXISTS groups(id SERIAL NOT NULL PRIMARY KEY, name varchar(20));
CREATE TABLE IF NOT EXISTS courses(id SERIAL NOT NULL PRIMARY KEY, name varchar(20), description varchar(50));
CREATE TABLE IF NOT EXISTS students(id SERIAL NOT NULL PRIMARY KEY, group_id INT, name varchar(20), lastName varchar(20), FOREIGN KEY(group_id) REFERENCES groups(id));
